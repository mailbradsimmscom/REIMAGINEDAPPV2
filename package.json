{
  "name": "reimaginedappv2",
  "version": "0.1.0",
  "private": true,
  "description": "Enterprise app scaffold (Node 20, ESM)",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "node --watch ./src/start.js",
    "dev:web": "node --watch ./src/start.js",
    "dev:worker": "node --watch ./src/start-job-processor.js",
    "dev:all": "concurrently -n web,worker -c auto \"npm:dev:web\" \"npm:dev:worker\"",
    "start": "node ./src/start.js",
    "start:worker": "node ./src/start-job-processor.js",
    "test": "node tests/run-tests-with-sidecar.js",
    "test:unit": "node --test tests/unit/",
    "test:all": "node --test tests/",
    "test:watch": "node --test --watch tests/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:baseline": "./tests/run-baseline.sh",
    "test:no-sidecar": "node --test tests/integration/",
    "test:performance": "node scripts/performance-monitor.js",
    "test:ci": "npm run test:no-sidecar && npm run test:performance",
    "zod:coverage": "node scripts/zod-coverage.mjs",
    "graph:js": "npx depcruise src --output-type text > docs/app-deps.txt",
    "graph:py": "~/.local/bin/pydeps python-sidecar/app -T svg -o docs/sidecar-deps.svg --noshow",
    "graph:madge": "madge --extensions js --image docs/app-madge.png src",
    "graph:all": "npm run graph:madge && npm run graph:py",
    "docs:generate": "npm run graph:all && echo 'Dependency graphs generated in docs/'",
    "docs:clean": "rm -rf docs/auto && mkdir -p docs/auto/graphs",
    "docs:js": "documentation build \"src/**/*.js\" -f md -o docs/auto/js.md || true",
    "docs:routes": "node scripts/dump-routes.mjs > docs/auto/routes.mdx",
    "docs:deps:dot": "depcruise --include-only \"^src\" --output-type dot src | dot -Tsvg -o docs/auto/graphs/deps.svg",
    "docs:deps:page": "node scripts/deps-page.mjs",
    "docs:openapi": "node scripts/gen-openapi.mjs",
    "docs:all": "npm run docs:clean && npm run docs:js && npm run docs:routes && npm run docs:deps:dot && npm run docs:deps:page && npm run docs:openapi"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "@supabase/supabase-js": "2.56.0",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "16.6.1",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-list-endpoints": "^7.1.1",
    "fast-glob": "^3.3.3",
    "helmet": "^8.1.0",
    "openai": "^5.19.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "concurrently": "^9.0.0",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "madge": "^8.0.0",
    "supertest": "^7.1.4"
  }
}
